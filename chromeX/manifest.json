{
  "manifest_version": 2,

  "name": "JiReM",
  "description": "Opens a Jira issue of ReM",
  "version": "1.0.2",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
